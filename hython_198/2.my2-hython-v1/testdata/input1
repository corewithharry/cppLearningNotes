    print b;
    def c = a * 2 + b;
    def i1, i2, i3, i4;
    {
        def d = a + 12;
        a = 500;
        print a;
        def a = 10000;
        def e = a + 12;
        a = 1000;
        def g = a + 20;
        print g;
    }
    def f = a + 12;
    if (f > 1) {
        def a = 6666;
        print a;
    }
    print a;
    def i;
    for (i = 1; i <= 5; i = i + 1) {
        print i;
    }
}

